{
  "openrpc": "1.0.0-rc1",
  "info": {
    "version": "1.0.0",
    "title": "Process Inscription",
    "license": {
      "name": "MIT"
    }
  },
  "servers": [
    {
      "url": "ws://${host}:${port}/designer/ivy-inscription-lsp",
      "variables": {
        "host": {
          "description": "host where the AxonIvy Designer is running",
          "default": "localhost"
        },
        "port": {
          "description": "port where an AxonIvy Designer is running",
          "default": "8081"
        }
      }
    }
  ],
  "methods": [
    {
      "name": "initialize",
      "tags": [
        {
          "name": "editor"
        }
      ],
      "params": [],
      "result": {
        "name": "initialized",
        "schema": {
          "type": "boolean"
        }
      }
    },
    {
      "name": "data",
      "summary": "All inscription data of a process element",
      "tags": [
        {
          "name": "editor"
        }
      ],
      "params": [
        {
          "$ref": "#/components/contentDescriptors/InscribeArg"
        }
      ],
      "result": {
        "name": "inscription",
        "description": "All inscription data",
        "schema": {
          "$ref": "#/components/schemas/InscriptionRequest"
        }
      }
    },
    {
      "name": "saveData",
      "summary": "sets the inscription data of a process element",
      "tags": [
        {
          "name": "editor"
        }
      ],
      "params": [
        {
          "name": "saveDataArgs",
          "required": true,
          "schema": {
            "type": "object",
            "properties": {
              "pid": {
                "type": "string"
              },
              "data": {
                "schema": {
                  "$ref": "#/components/schemas/Data"
                }
              }
            }
          }
        }
      ],
      "result": {
        "name": "validation message",
        "description": "ALPHA: validation message struct is yet not generated anywhere",
        "schema": {
          "type": "array"
        }
      }
    },
    {
      "name": "meta/start/dialogs",
      "summary": "Startable Dialogs",
      "tags": [
        {
          "name": "meta"
        }
      ],
      "params": [
        {
          "$ref": "#/components/contentDescriptors/InscribeArg"
        }
      ],
      "result": {
        "name": "starts",
        "description": "Dialog Starts",
        "schema": {
          "type": "array",
          "items": {
            "$ref": "#/components/schemas/CallableStart"
          }
        }
      }
    },
    {
      "name": "meta/out/map",
      "summary": "Mappings",
      "tags": [
        {
          "name": "meta"
        }
      ],
      "params": [
        {
          "$ref": "#/components/contentDescriptors/InscribeArg"
        }
      ],
      "result": {
        "name": "mapping info",
        "description": "output mapping table info",
        "schema": {
          "$ref": "#/components/schemas/MappingInfo"
        }
      }
    },
    {
      "name": "meta/workflow/roles",
      "summary": "roles accessible from the elements-project",
      "tags": [
        {
          "name": "meta"
        }
      ],
      "params": [
        {
          "$ref": "#/components/contentDescriptors/InscribeArg"
